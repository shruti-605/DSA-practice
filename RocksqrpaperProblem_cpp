#include <iostream>
using namespace std;
int main() {
    // your code goes here
    int tt;
    cin>>tt;
    while(tt--){
        int n;
        cin>>n;
        int r1,p1,s1,r2,p2,s2;
        cin>>r1>>p1>>s1>>r2>>p2>>s2;
        int max_win=min(r1,s2)+min(p1,r2)+min(s1,p2);
        int cnt_to_win=(n+1)/2;//we are using here ceal value;
        if(max_win>cnt_to_win){
            cout<<"Yes"<<endl;
        }else{
            cout<<"No"<<endl;
        }
        
    }
    return 0;
}
